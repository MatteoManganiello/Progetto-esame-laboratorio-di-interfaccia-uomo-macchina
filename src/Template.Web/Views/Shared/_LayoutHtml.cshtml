<!DOCTYPE html>
<html lang="@this.CurrentCulture().Name" class="@ViewData["FullscreenClass"]">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewData["Title"] - OnModuli</title>
    <meta name="description" content="@ViewData["Description"]">
    
    @RenderSection("googlehead", false)
    @RenderSection("metas", false)

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link href="~/node_modules/@("@")fortawesome/fontawesome-free/css/all.min.css" asp-append-version="true" rel="stylesheet" />
    <link href="~/css/bundle-global.min.css" asp-append-version="true" rel="stylesheet" />
    
    @RenderSection("styles", false)
</head>
<body>
    @RenderSection("googlebody", false)
    
    @RenderBody()

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

    <script type="text/javascript">
        var clientLocale = '@this.CurrentCulture().Name';
    </script>
    
    <environment include="Development">
        <script src="~/js/bundle-global.js" asp-append-version="true"></script>
    </environment>
    <environment include="Staging,Production">
        <script src="~/js/bundle-global.min.js" asp-append-version="true"></script>
    </environment>
    
    @RenderSection("scripts", required: false)
</body>
</html>